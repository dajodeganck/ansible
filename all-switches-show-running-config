---
- name: "Show Running Configuration on Switches"
  hosts: switches
  gather_facts: no
  become: yes
  vars:
    ansible_become_pass: Test1231
  tasks:
    - name: Show Running Config
      cisco.ios.ios_command:
        commands: show running-config
      register: output

    - name: Print Running Config
      debug:
        msg: "{{ output.stdout }}"
